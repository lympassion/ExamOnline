<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.loti.dao.mapper.StuMapper">
    <select id="getAllStudent" resultType="com.loti.dao.pojo.Entity.User.Student">
        select * from student
    </select>
    <select id="selectById" parameterType="java.lang.String" resultType="com.loti.dao.pojo.Entity.User.Student">
        select * from student where id = #{id}
    </select>
</mapper>