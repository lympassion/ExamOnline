<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.loti.dao.mapper.CourseMapper">
    <select id="SelectAllCourse" resultType="com.loti.dao.pojo.Entity.Course">
        select * from course
    </select>
    <select id="SelectCourseById" parameterType="int" resultType="com.loti.dao.pojo.Entity.Course">
        select * from course where (course_id) = #{id}
    </select>
    <insert id="InsertCourse"  parameterType="com.loti.dao.pojo.Entity.Course">
        insert into course (course_id,course_name) VALUES (#{courseId},#{courseName})
    </insert>
</mapper>